# TTB Pipeline Environment Configuration
# Copy this file to .env and update values for your environment

# Deployment Environment (dev, staging, prod)
DAGSTER_DEPLOYMENT=dev

# S3 Configuration
TTB_S3_BUCKET=ciq-dagster
TTB_RAW_PREFIX=1-ttb-raw-data
TTB_PROCESSED_PREFIX=2-ttb-processed-data
TTB_DIMENSIONAL_PREFIX=3-ttb-dimensional-data

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Processing Configuration
TTB_MAX_SEQUENCE=100
TTB_EXTRACTION_MAX_SEQUENCE=100
TTB_REQUEST_DELAY=0.5
TTB_MAX_RETRIES=3
TTB_SSL_VERIFY=false

# Data Quality Settings
TTB_ENABLE_QUALITY_CHECKS=true
TTB_ENABLE_DEDUP=true
TTB_CREATE_PARTITIONED=true

# Dimensional Modeling Settings
TTB_DATE_START_YEAR=2015
TTB_DATE_END_YEAR=2030
TTB_SURROGATE_KEYS=true

# Development Settings (dev environment only)
TTB_DEBUG_LOGGING=true
TTB_SAMPLE_DATA_ONLY=false